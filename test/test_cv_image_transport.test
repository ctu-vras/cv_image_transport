<launch>
    <test test-name="test_cv_image_transport" pkg="cv_image_transport" type="test_cv_image_transport" />
    <node name="republish_cv" pkg="image_transport" type="republish" args="raw cv">
        <remap from="in" to="/spot/camera/hand_color/image" />
        <remap from="out" to="republished" />
    </node>
    <node name="republish_raw" pkg="image_transport" type="republish" args="cv raw">
        <remap from="in" to="republished" />
        <remap from="out" to="republished2" />
    </node>
</launch>